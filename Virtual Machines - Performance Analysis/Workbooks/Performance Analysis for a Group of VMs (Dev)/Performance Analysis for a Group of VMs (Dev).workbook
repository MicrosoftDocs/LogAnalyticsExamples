            "version": "KqlParameterItem/1.0",
            "name": "virtualMachinesSnippet",
            "type": 1,
            "query": "{\"version\":\"1.0.0\",\"content\":\"\\\"| where _ResourceId in ({VirtualMachines})\\\"\",\"transformers\":null}",
