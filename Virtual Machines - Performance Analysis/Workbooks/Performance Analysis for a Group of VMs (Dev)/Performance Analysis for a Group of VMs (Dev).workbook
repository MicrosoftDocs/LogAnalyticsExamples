            "name": "vmSnippet",
            "type": 1,
            "value": "",
            "isHiddenWhenLocked": true,
            "criteriaData": [
              {
                "condition": "if (virtualMachinesValue == '*'), result = virtualMachinesSnippet",
                "criteriaContext": {
                  "leftOperand": "virtualMachinesValue",
                  "operator": "==",
                  "rightValType": "static",
                  "rightVal": "*",
