    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "let data = union pageViews, customEvents\r\n| where timestamp {TimeRange}\r\n| where name in ({Operations}) or '*' in ({Operations});\r\ndata\r\n| summarize Users = dcount(user_Id), Sessions = dcount(session_Id), Activities = count() by name, itemType\r\n| extend name2 = iff(itemType == 'customEvent', strcat('‚ö° ', name), strcat('üåê ', name))\r\n| extend rank = 2\r\n| union (data\r\n    | summarize Users = dcount(user_Id), Sessions = dcount(session_Id), Activities = count()\r\n    | extend name = '', name2 = 'üî∏üî∏ Overall', rank = 1)\r\n| order by rank asc, Users desc, Activities desc\r\n| project Activity = name2, ['Unique Users'] = Users, ['Unique Sessions'] = Sessions, ['Activity Count'] = Activities, name\r\n",
        "showQuery": false,
        "size": 0,
        "aggregation": 0,
        "showAnnotations": false,
        "exportFieldName": "name",
        "exportParameterName": "SelectedOperation",
        "showAnalytics": false,
        "title": "Usage Summary",
        "resourceType": "microsoft.insights/components",
